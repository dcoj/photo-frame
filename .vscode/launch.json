{
  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "${defaultBuildTask}",
      "type": "probe-rs-debug",
      "request": "launch",
      "name": "debug",
      "flashingConfig": {
        "flashingEnabled": true,
      },
      "chip": "esp32s3",
      "coreConfigs": [
        {
            "coreIndex": 0,
            "rttEnabled": true,
            "programBinary": "./target/xtensa-esp32s3-none-elf/debug/photo-frame"
        }
      ],
    }
  ]
}